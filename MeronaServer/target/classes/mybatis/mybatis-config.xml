<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias type="entity.User" alias="user"/>
		<typeAlias type="entity.Todos" alias="todos"/>
		<typeAlias type="entity.Relationship" alias="relationship"/>
		<typeAlias type="entity.Address" alias="address"/>
		<typeAlias type="entity.Noti" alias="noti"/>
	</typeAliases>
	
	<!--쿼리문 -->
	<mappers>
		<mapper resource="mybatis/user-mapping.xml"/>
		<mapper resource="mybatis/todo-mapping.xml"/>
		<mapper resource="mybatis/friend-mapping.xml"/>
		<mapper resource="mybatis/noti-mapping.xml"/>
	</mappers>
</configuration>
